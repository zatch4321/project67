<div class="container mx-auto px-4 my-20" data-aos="fade-up">
  <h2 class="text-3xl font-bold bg-gradient-to-r from-primary-400 to-secondary-400 text-transparent bg-clip-text mb-8">Submitted Applications</h2>
  <div class="overflow-x-auto">
    <table class="min-w-full bg-gray-800/50 backdrop-blur-sm rounded-xl border border-gray-700">
      <thead>
        <tr class="border-b border-gray-700">
          <th class="py-4 px-6 text-left text-gray-300">Name</th>
          <th class="py-4 px-6 text-left text-gray-300">Email</th>
          <th class="py-4 px-6 text-left text-gray-300">Skills</th>
          <th class="py-4 px-6 text-left text-gray-300">Message</th>
          <th class="py-4 px-6 text-left text-gray-300">Actions</th>
        </tr>
      </thead>
      <tbody id="applicationsTable" class="divide-y divide-gray-700"></tbody>
    </table>
  </div>
</div>